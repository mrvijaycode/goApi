    "paths": {
        "/bitgoAccounts": {
            "get": {
                "description": "Display the wallet accounts.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "root"
                ],
                "summary": "Get Bitgo wallet accounts.",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/createalgoaccount": {
            "get": {
                "description": "Get the status of server.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "root"
                ],
                "summary": "Create a new algo account.",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/fundAccount": {
            "post": {
                "description": "Funding the account",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "root"
                ],
                "summary": "Fund account",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Add transaction details",
                        "name": "account",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/pagoentities": {
            "get": {
                "description": "get the status of server.",
